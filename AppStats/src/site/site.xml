<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="Maven">

		
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.3.0</version>
  </skin>

  <custom>
    <fluidoSkin>
      <topBarEnabled>true</topBarEnabled>
      <sideBarEnabled>false</sideBarEnabled>
      <topBarIcon>
        <name>Maven Fluido Skin</name>
        <alt>Maven Fluido Skin</alt>
        <src>/images/bell-curve-extra-small.png</src>
        <href>/index.html</href>
      </topBarIcon>
      <topBarContainerStyle>width: 75%;</topBarContainerStyle>
      <navBarStyle>navbar-inverse</navBarStyle>
      <!-- Git Hub Fork -->
      <gitHub>
        <projectId>wrightm/AppStats</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>red</ribbonColor>
      </gitHub>
      <!-- Facebook like -->
      <facebookLike />
      <!-- Google One -->
      <googlePlusOne />
      
    </fluidoSkin>
  </custom>
  
  <body>
	 <breadcrumbs>
            <item name="Developer Guide" href="developer-guide.html"/>
            <item name="User Guide" href="user-guide.html"/>
     </breadcrumbs>
     
    <menu name="Guides">
      <item name="Developer" href="developer-guide.html"/>
      <item name="User" href="user-guide.html"/>
      <item name="FAQs" href="faq.html"/>
    </menu>

  </body>
</project>